{
  "hosting": {
    "public": "public",
    "rewrites":
    [{
      "source": "**",
      "function": "app"
    }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "functions": {
    "source": "functions",
    "//": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ]
  }
}
